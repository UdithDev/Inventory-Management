<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.DashboardController">
   <children>
      <AnchorPane fx:id="pane" opacity="0.6" prefHeight="600.0" prefWidth="450.0" style="-fx-background-color: gray;">
         <children>
            <Button fx:id="btnCustomerManager" layoutX="75.0" layoutY="118.0" mnemonicParsing="false" onAction="#btnCustomerManagerOnAction" prefHeight="39.0" prefWidth="210.0" style="-fx-background-color: black; -fx-background-radius: 18;" text="Customer Manager" textFill="WHITE">
               <font>
                  <Font name="Bernard MT Condensed" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="btnItemManager" layoutX="75.0" layoutY="204.0" mnemonicParsing="false" onAction="#btnItemManagerOnAction" prefHeight="39.0" prefWidth="210.0" style="-fx-background-color: black; -fx-background-radius: 18;" text="Item Manager" textFill="WHITE">
               <font>
                  <Font name="Bernard MT Condensed" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="btnOrderManager" layoutX="75.0" layoutY="287.0" mnemonicParsing="false" onAction="#btnOrderManagerOnAction" prefHeight="39.0" prefWidth="210.0" style="-fx-background-color: black; -fx-background-radius: 18;" text="Order Manager" textFill="WHITE">
               <font>
                  <Font name="Bernard MT Condensed" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="btnPlaceOrder" layoutX="75.0" layoutY="369.0" mnemonicParsing="false" onAction="#btnPlaceOrderOnAction" prefHeight="39.0" prefWidth="210.0" style="-fx-background-color: black; -fx-background-radius: 18;" text="Place Order" textFill="WHITE">
               <font>
                  <Font name="Bernard MT Condensed" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
